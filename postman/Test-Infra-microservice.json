      "name": "2. Session Management (Redis)",
      "description": "Test distributed session management with Redis",
      "item": [
        {
          "name": "Create User Session",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Session created successfully', function () {",
                  "    pm.response.to.have.status(200);",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.message).to.eql('Login successful');",
                  "});",
                  "",
                  "pm.test('Session ID is returned', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('sessionId');",
                  "    pm.expect(jsonData.sessionId).to.not.be.empty;",
                  "});",
                  "",
                  "pm.test('User ID is correct', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.userId).to.eql('microservice_user');",
                  "});",
                  "",
                  "// Save session info",
                  "var jsonData = pm.response.json();",
                  "pm.collectionVariables.set('sessionId', jsonData.sessionId);",
                  "pm.collectionVariables.set('userId', jsonData.userId);",
                  "",
                  "console.log('âœ… SESSION CREATED');",
                  "console.log('   Session ID:', jsonData.sessionId);",
                  "console.log('   User ID:', jsonData.userId);"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"microservice_user\",\n  \"password\": \"test_password_123\"\n}"
            },
            "url": {
              "raw": "{{apiGatewayUrl}}/api/session/login",
